{
  "name": "@flux-blockchain/sdk",
  "version": "1.0.0",
  "description": "Official TypeScript SDK for Flux Blockchain",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build && npm test",
    "docs": "typedoc --out docs/api src/index.ts"
  },
  "keywords": [
    "flux",
    "blockchain",
    "sdk",
    "ethereum",
    "polygon",
    "web3",
    "typescript"
  ],
  "author": "Flux Blockchain Team",
  "license": "MIT",
  "dependencies": {
    "ethers": "^6.9.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jeromwolf/flux-blockchain.git"
  },
  "bugs": {
    "url": "https://github.com/jeromwolf/flux-blockchain/issues"
  },
  "homepage": "https://github.com/jeromwolf/flux-blockchain#readme"
}